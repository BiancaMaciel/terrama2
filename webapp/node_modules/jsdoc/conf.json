{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["public/javascripts"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "docs"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "systemName": "TerraMA²",
        "footer": "TerraMA²",
        "copyright": "2015",
        "navType": "vertical",
        "linenums": true,
        "collapseSymbols": true,
        "inverseNav": false
    },
    "opts": {
        "template": "node_modules/jsdoc/templates/terrama2",
        "encoding": "utf8",
        "destination": "docs",
        "recurse": true,
        "private": true
    }
}
